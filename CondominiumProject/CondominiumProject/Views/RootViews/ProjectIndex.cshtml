    <div class="container text-center">
        <div class="row">
            <div class="col-sm-6 mb-3 mb-sm-0">
                <div class="card">
                    <div class="card-body">
                        @if (Model != null)
                        {
                            <img src="../@Model.Logo" class="card-img-top" style="width: 18rem;">
                            <br>
                            <br>
                            <h5 class="card-title">@Model.Name</h5>
                        }
                    </div>
                </div>
            </div>
            <div class="col-sm-6">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Visits</h5>
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th scope="col">House</th>
                                    <th scope="col">Date</th>
                                    <th scope="col">Hour</th>
                                    <th scope="col">Name</th>
                                    <th scope="col"></th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (ViewBag.VisitDetailsList != null)
                                {
                                    foreach (var visit in ViewBag.VisitDetailsList)
                                    {
                                        <tr>
                                            <th scope="row">@visit.IDHabitation</th>
                                            <td>@visit.VisitDate.ToString("dd-MM-yyyy")</td>
                                            <td>@visit.VisitTime</td>
                                            <td>@visit.FirstName @visit.LastName</td>
                                            <td>
                                                <button type="button" class="btn btn-info" data-bs-toggle="modal" data-bs-target="#exampleModal_@visit.IDHabitation">Informacion</button>
                                            </td>
                                        </tr>
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>

                @foreach (var visit in ViewBag.VisitDetailsList)
                {
                    <div class="modal" id="exampleModal_@visit.IDHabitation" data-bs-backdrop="static" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title fs-5" id="exampleModalLabel">Visit Information</h5>
                                </div>
                                <div class="modal-body">
                                    <div class="card">
                                        <ul class="list-group list-group-flush">
                                            <li class="list-group-item"><h5 class="card-title">@visit.FirstName @visit.LastName</h5></li>
                                            <li class="list-group-item"><p class="card-text"><p class="tile_card">Visit DNI<br>@visit.DNI</p></li>
                                            <li class="list-group-item"><p class="card-text"><p class="tile_card">House<br># @visit.IDHabitation</p></li>
                                            <li class="list-group-item">
                                                <table class="table table-striped">
                                                    <thead>
                                                        <tr>
                                                            <th scope="col">Marc</th>
                                                            <th scope="col">Plate</th>
                                                            <th scope="col">Model</th>
                                                            <th scope="col">Color</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr>
                                                            <th scope="row">@visit.VehicleMarc</th>
                                                            <td>@visit.VehiclePlate</td>
                                                            <td>@visit.VehicleModel</td>
                                                            <td>@visit.VehicleColor</td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </li>
                                            <li class="list-group-item">
                                                <table class="table table-striped">
                                                    <thead>
                                                        <tr>
                                                            <th scope="col">Date</th>
                                                            <th scope="col">Hour</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr>
                                                            <td>@visit.VisitDate.ToString("yyyy-MM-dd")</td>
                                                            <td>@visit.VisitTime</td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cerrar</button>
                                </div>
                            </div>
                        </div>
                    </div>
                }

            </div>


            <div class="col-sm-6 mb-3 mb-sm-0">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Users Habitation</h5>
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th scope="col">Number House</th>
                                    <th scope="col">DNI</th>
                                    <th scope="col">Name</th>
                                    <th scope="col"></th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (ViewBag.HabitationDetailsList != null)
                                {
                                    foreach (var habitationDetails in ViewBag.HabitationDetailsList)
                                    {
                                        <tr>
                                            <th scope="row">@habitationDetails.IDHabitation</th>
                                            <td>@habitationDetails.DNI</td>
                                            <td>@habitationDetails.FirsName @habitationDetails.LastName</td>
                                            <td><button type="button" class="btn btn-danger">Delete</button></td>
                                        </tr>
                                    }
                                }
                            </tbody>
                        </table>
                        <button type="button" class="btn btn-primary">Add</button>
                    </div>
                </div>
            </div>

        </div>
    </div>

    





