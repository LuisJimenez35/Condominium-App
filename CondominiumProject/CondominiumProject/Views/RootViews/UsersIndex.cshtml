    <div class="text-center">
        @if (ViewBag.RootHasAccessToProjects)
        {
            <div class="row">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th scope="col">Name</th>
                            <th scope="col">Principal Telephone</th>
                            <th scope="col">Email</th>
                            <th><button type="submit" style="font-size:90%; font-weight:normal" class="btn btn-secondary" data-bs-toggle="modal" data-bs-target="#exampleModal">Add Condominum</button></th>
                            <th>
                                <input type="text" class="form-control" id="myInput" placeholder="Search Condominum" aria-label="Search..">
                            </th>
                        </tr>
                    </thead>
                    <tbody id="myTable">
                        @foreach (var user in ViewBag.UsersList)
                        {
                            <tr>
                                <td>@user.FirsName @user.LastName</td>
                                <td>@user.Telephone1</td>
                                <td>@user.Email</td>
                                <td><button type="button" style="font-size:12px" class="btn btn-info" data-bs-toggle="modal" data-bs-target="#exampleModal1_@user.IdUser" data-id="@user.IdUser" dara-dni="@user.DNI" data-firsname="@user.FirsName" data-lastname="@user.LastName" data-telephone1="@user.Telephone1" data-telephone2="@user.Telephone2" data-email="@user.Email" data-picture="@user.Picture" data-password="@user.Password">More Information</button></td>
                                <td><button type="button" style="font-size:12px" class="btn btn-danger" data-bs-toggle="modal" data-bs-target="#staticBackdrop_@user.IdUser" data-id="@user.IdUser" data-firsname="@user.FirsName" data-lastame="LastName">Delete</button></td>
                            </tr>

                            <!-- Modal -->
                            <div class="modal fade" id="exampleModal1_@user.IdUser" data-bs-backdrop="static" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                                <div class="modal-dialog modal-dialog-centered">
                                    <div class="modal-content rounded shadow">
                                        <div class="modal-header bg-primary text-white">
                                            <h1 class="modal-title fs-5" id="exampleModalLabel">Condominium Information</h1>
                                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                        </div>
                                        <div class="modal-body">
                                            <img src="../@user.Picture" style="width: 16rem; border-radius: 8px; box-shadow: 0 4px 8px rgba(0,0,0,0.1);" class="card-img-top mb-3">

                                            <div class="card">
                                                <ul class="list-group list-group-flush">
                                                    <li class="list-group-item"><h5 class="card-title">@user.FirsName @user.LastName</h5></li>
                                                    <li class="list-group-item"><p style="color:black" class="card-text tile_card">DNI: @user.DNI</p></li>
                                                    <li class="list-group-item"><p style="color:black" class="card-text tile_card">Telephones: <br>@user.Telephone1<br>@user.Telephone2</p></li>
                                                    <li class="list-group-item"><p style="color:black" class="card-text tile_card">Email: @user.Email</p></li>
                                                    <li class="list-group-item"><p style="color:black" class="card-text tile_card">Password: @user.Password</p></li>
                                                </ul>
                                                <div class="card-footer">
                                                    Assigned by: <br>

                                                    @if (string.IsNullOrEmpty(@user.ProjectName) && @user.IDHabitation == 0)
                                                    {
                                                        <form asp-controller="HabitationProjectC" asp-action="AsignHouse">
                                                            <div class="row">
                                                                <div class="col">
                                                                    <input name="IDHabitation" type="number" class="form-control" placeholder="IDHabitation" required>
                                                                </div>
                                                                <div class="col">
                                                                    <select name="IDProyect" class="form-select" aria-label="Default select example">
                                                                        @foreach (var habitational_project in ViewBag.ProjectLists)
                                                                        {
                                                                            <option value="@habitational_project.IdProject">@habitational_project.Name</option>
                                                                        }
                                                                    </select>
                                                                </div>

                                                                <input name="email" type="hidden" class="form-control" value="@ViewBag.email" />
                                                                <input name="email1" type="hidden" class="form-control" value="@user.Email" />
                                                                <input name="IdUser" type="hidden" class="form-control" value="@user.IdUser" />
                                                                <input name="username" type="hidden" class="form-control" value="@user.FirsName"/>
                                                                <br><br>
                                                                <button type="submit" class="btn btn-success">Assign</button>
                                                            </div>
                                                        </form>
                                                    }
                                                    else
                                                    {
                                                        @user.ProjectName <p>Habitation:</p> @user.IDHabitation
                                                    }

                                                </div>
                                            </div>
                                        </div>
                                        <div class="modal-footer">
                                            <button type="submit" class="btn btn-primary">Edit Condominium</button>
                                        </div>
                                    </div>
                                </div>
                            </div>


                            <!-- Modal Eliminar -->
                            <div class="modal fade" id="staticBackdrop_@user.IdUser" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
                                <div class="modal-dialog">
                                    <div class="modal-content">
                                        <form asp-controller="UsersC" asp-action="DeleteUser">
                                            <div class="modal-header">
                                                <h1 class="modal-title fs-5" id="staticBackdropLabel">Delete Condominum</h1>
                                            </div>
                                            <div class="modal-body">
                                                <p>¿Are you sure you want to delete Condominum <br><h2 class="font-bold">@user.FirsName @user.LastName?</h2> </p>
                                                <input name="IdUser" type="hidden" id="modalRootId_@user.IdUser" value="@user.IdUser" />
                                                <input name="email" type="hidden" class="form-control" value="@ViewBag.email" />
                                            </div>
                                            <div class="modal-footer">
                                                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                                                <button type="submit" class="btn btn-danger">Delete</button>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        }
                    </tbody>
                </table>
            </div>
        }
    </div>

    <!-- Modal Crear Usuarios-->
    <div class="modal fade" id="exampleModal" data-bs-backdrop="static" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <form asp-controller="UsersC" asp-action="CreateUser">
                    <div class="modal-header">
                        <h1 class="modal-title fs-5" id="exampleModalLabel">Crete Condominum
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col">
                                <input name="DNI" type="text" class="form-control" placeholder="DNI" required>
                            </div>
                            <div class="col">
                                <input name="FirsName" type="text" class="form-control" placeholder="First Name" required>
                            </div>
                            <div class="col">
                                <input name="LastName" type="text" class="form-control" placeholder="Last Name" required>
                            </div>
                            <input name="email1" type="hidden" class="form-control" value="@ViewBag.email" />
                        </div>
                        <br>
                        <div class="row">
                            <div class="col">
                                <input name="Telephone1" type="text" class="form-control" placeholder="Telephone 1" required>
                            </div>
                            <div class="col">
                                <input name="Telephone2" type="text" class="form-control" placeholder="Telephone 2">
                            </div>
                        </div>
                        <br>
                        <div class="row">
                            <div class="col">
                                <input name="Email" type="email" class="form-control" placeholder="Email" required>
                            </div>
                            <div class="col">
                                <input name="Password" type="password" class="form-control" id="floatingPassword" placeholder="Password" required>
                            </div>
                        </div>
                        <br>
                        <div class="col">
                            <input name="Picture" class="form-control" type="file" id="formFile">
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                        <button type="submit" class="btn btn-success">Create Condominum</button>
                    </div>
                </form>
            </div>
        </div>
    </div>

